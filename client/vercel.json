{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "build": {
    "env": {
      "VITE_API_BASE_URL": "@api_base_url"
    }
  },
  "env": {
    "VITE_API_BASE_URL": "https://your-api-domain.com/api"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
